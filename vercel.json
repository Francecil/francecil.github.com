{
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/:any",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, maxage=2592000, s-maxage=2592000, stale-while-revalidate=31536000"
        }
      ]
    }
  ]
}
